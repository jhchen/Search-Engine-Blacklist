{
	"name": "Search Engine Blacklist",
	"version": "2.5.5",
	"description": "Easily blacklist spam sites from ever appearing in your Google search results again.",
	"icons": { 	"16": "gear-16.png",
           		"48": "gear-48.png",
          		"128": "gear-128.png" },
	"content_scripts": [{
		"matches": ["http://www.google.com/search*", "http://www.google.com/"],
		"js": ["jquery-1.4.4.js", "parseUri.js", "blacklist.js"],
		"run_at": "document_end"
	}],
	"options_page": "options.html",
	"background_page": "background.html"
}
